    <script>
        var homeSlick = $('.hero-slides').slick({
            dots: false,
            infinite: true,
            speed: 300,
            slidesToShow: 1,
            adaptiveHeight: true
        });

        $(function () {
            var oldSlick = null;

            $("[slickTo]").each(function () {
                $(this).mouseenter(function () {
                    if (oldSlick) {
                        oldSlick.removeClass("active");
                    }

                    var slide = $(this).attr("slickTo");
                    $(this).addClass("active");
                    $('.slides').slick('slickGoTo', slide);
                    oldSlick = $(this);
                });


            });
        });


        var blogSlick = $('.block-feed-slider').slick({
            dots: true,
            infinite: true,
            speed: 300,
            slidesToShow: 2,
            slidesToScroll: 2,
            adaptiveHeight: true,
            responsive: [
                {
                    breakpoint: 960,
                    settings: {
