{# obory #}
<div class="m-section l-feed_four">

    <header class="m-section--header">
        <div class="l-full">
            <h2 class="m-section--hed mm-big">Reference / <span class="color-primary">archiv referencí</span></h2>
        </div>
    </header>


    <div class="m-section--content" -keep-height>
        <div class="l-single">
            {% set tClass = ['m-card_image mm-low', ' mm-color-', a]|join('') %}
            {% include 'subtemplates/base-item.twig' with {'contentType': 'm-story', 'class': tClass } %}
        </div>
        <div class="l-single">
            {% set tClass = ['m-card_image mm-tall', ' mm-color-', a]|join('') %}
            {% include 'subtemplates/base-item.twig' with {'contentType': 'm-story', 'class': tClass } %}
        </div>
        {% for a in 1..6 %}
            <div class="l-single">
                {% set tClass = ['m-card_image', ' mm-color-', a]|join('') %}
                {% include 'subtemplates/base-item.twig' with {'contentType': 'm-story', 'class': tClass } %}
            </div>
        {% endfor %}
    </div>


    <footer class="m-section--footer">
        <div class="l-half">
            <div class="m-section--top"><a href="">Nahoru <i class="fa fa-arrow-up"></i></a></div>
        </div>
        <div class="l-half">
            <ul class="m-section--nav inline-right">
                <li><a href="" title="Novinky">Technická pomoc</a></li>
                <li><a href="">informace pro architekty</a></li>
            </ul>
        </div>
    </footer>


</div>

